#include <stdio.h>
#include<string.h>
int main() {
    char s[50];
    scanf("%s",s);
    int i;
    for(i=0;s[i]!='\0';i++){           // mOhaMMeD
        if(s[i]>=65 && s[i]<90)       //  MoHAmmEd
        s[i]=s[i]+32;
        else if(s[i]>=97 && s[i]<=122)
        s[i]=s[i]-32;
    }
    printf("%s",s);
    return 0;
}
