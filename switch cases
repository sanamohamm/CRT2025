#include <stdio.h>

int main() {
   int a,b,choice;
   scanf("%d %d %d",&a,&b,&choice);
   switch(choice){
       case 1: printf("%d",a+b);
       break;
         default: printf("invalid");
       case 2:
       printf("%d",a-b);
       break;
       case 3:printf("%d",a*b);
       break;
       case 4: printf("%d",a/b);
       break;
      
   }
    return 0;
}
