#include <stdio.h>
int main() {
    int i,n,esum=0,osum=0;
    printf("enter array size:");
    scanf("%d",&n);
    int a[n];
    printf("enter elements:");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        if(a[i]%2==0)
            esum=esum+a[i];
    else
    osum=osum+a[i];
    }
    printf(" odd numbers sum is:%d\n",esum);
    printf("even numbers sum is:%d",osum);
    return 0;
}
