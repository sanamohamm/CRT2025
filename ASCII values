#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char ch;
    scanf("%c",&ch);
    if(ch>='A' && ch<='Z'){
        printf("Upper");
    }
    else if(ch>=97 && ch<=122)    // ch<='a' && ch>='z'
{
        printf("Lower");
    }
    else if(ch>=48 && ch<=57)    // ch<='0' && ch>= '9'
{
        printf("Number");
    }
    else  
        printf("Symbol");
    return 0;
}
