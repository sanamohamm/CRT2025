#include <stdio.h>
#include<math.h>
int main() {
    int a,b,c;                                  
    scanf("%d%d %d",&a,&b,&c);
    if(a>b){
        if(a>c){
        printf("%d big",a);
        }
        else{ 
        printf("%d big",c);
        }
    }
        else if(b>c){
        printf("%d big",b);
        }
        return 0;
}
